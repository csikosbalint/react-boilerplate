{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "indent": [
      "error",
      2
    ]
  },
  "extends": [
    "airbnb-base"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}